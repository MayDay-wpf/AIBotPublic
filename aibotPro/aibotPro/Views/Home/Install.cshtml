        }

        .section {
            margin-bottom: 20px;
        }

        .log-area {
            height: 200px;
            overflow-y: scroll;
            border: 1px solid #ccc;
            padding: 10px;
        }

        .form-group {
            margin-bottom: 10px;
            border: 1px solid #ccc;
            padding: 15px;
            border-radius: 10px;
        }

            .form-group label {
                display: block;
                margin-bottom: 5px;
            }

            .form-group input {
                width: 200px;
                padding: 5px;
            }

            .form-group button {
                padding: 5px 10px;
            }

        .content {
            width: 50%;
            margin: 0 auto;
        }
    </style>
</head>
